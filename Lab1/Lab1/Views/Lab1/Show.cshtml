
@{
    ViewBag.Title = "Show";
}
<h2>Show</h2>

<table>
    <tr>
        <td>用户名：  </td>
        <td>@Request["username"]</td>
    </tr>

    <tr>
        <td>密码：  </td>
        <td>@Request["psd"]</td>
    </tr>

    <tr>
        <td>性别：  </td>
        <td>@Request["sex"]</td>
    </tr>

    <tr>
        <td>籍贯：  </td>
        <td>@Request["prov"]</td>
    </tr>

    <tr>
        <td>邮箱：  </td>
        <td>@Request["eml"]</td>
    </tr>

    <tr>
        <td>手机号：  </td>
        <td>@Request["tel"]</td>
    </tr>

    <tr>
        <td>专业特长：  </td>
        <td>@Request["speciality"] </td>
    </tr>

    <tr>
        <td>业余爱好：  </td>
        <td>@Request["hobbies"]</td>
    </tr>

    <tr>
        <td>个人图片： </td>
        <td>
            @*<img src=@Request["pic"]/>*@ 
            @Request["pic"]
        </td>
    </tr>

    <tr>
        <td>出生年月：  </td>
        <td>@Request["birthday"]</td>
    </tr>

    <tr>
        <td>备注信息：</td>
        <td>@Request["notes"]</td>
    </tr>

</table>